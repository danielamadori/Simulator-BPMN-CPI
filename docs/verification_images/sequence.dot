digraph G {
rankdir=LR;
splines=ortho;
nodesep=0.5;
ranksep=0.8;
compound=true;
node [fontname="Arial"];
edge [fontname="Arial"];

subgraph cluster_R2 {
label="R2";
style="rounded";
color="black";
bgcolor="white";
penwidth=1.5;
"t2" [shape=box label="R2" style="rounded,filled" fillcolor="white" color="black" fontcolor="black" penwidth=1.5 fontsize=14];
}

subgraph cluster_R1 {
label="R1";
style="rounded";
color="black";
bgcolor="white";
penwidth=1.5;
"t1" [shape=box label="R1" style="rounded,filled" fillcolor="white" color="black" fontcolor="black" penwidth=1.5 fontsize=14];
}
"start" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"p1" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"end" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"start" -> "t1" [color="black" arrowhead="normal"];
"t1" -> "p1" [color="black" arrowhead="normal"];
"p1" -> "t2" [color="black" arrowhead="normal"];
"t2" -> "end" [color="black" arrowhead="normal"];
}