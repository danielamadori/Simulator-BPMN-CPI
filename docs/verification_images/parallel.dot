digraph G {
rankdir=LR;
splines=ortho;
nodesep=0.5;
ranksep=0.8;
compound=true;
node [fontname="Arial"];
edge [fontname="Arial"];

subgraph cluster_R2 {
label="R2";
style="rounded";
color="black";
bgcolor="white";
penwidth=1.5;
"t_b" [shape=box label="R2" style="rounded,filled" fillcolor="white" color="black" fontcolor="black" penwidth=1.5 fontsize=14];
}

subgraph cluster_R1 {
label="R1";
style="rounded";
color="black";
bgcolor="white";
penwidth=1.5;
"t_a" [shape=box label="R1" style="rounded,filled" fillcolor="white" color="black" fontcolor="black" penwidth=1.5 fontsize=14];
}
"p_a_end" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"end" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"start" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"p_b_end" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"p_b" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"p_a" [shape=circle label="▷" fontsize=24 style="filled" fillcolor="white" penwidth=1.5 fontcolor="black" fixedsize=false width=0.6];
"split" [shape=triangle label="" style="filled" fillcolor="white" color="black" fontcolor="black" penwidth=1.5 fontsize=14 orientation=-90];
"join" [shape=triangle label="" style="filled" fillcolor="black" color="black" fontcolor="white" penwidth=1.5 fontsize=14 orientation=-90];
"t_b" -> "p_b_end" [color="black" arrowhead="normal"];
"start" -> "split" [color="black" arrowhead="normal"];
"t_a" -> "p_a_end" [color="black" arrowhead="normal"];
"split" -> "p_a" [color="black" arrowhead="normal"];
"p_b" -> "t_b" [color="black" arrowhead="normal"];
"p_a_end" -> "join" [color="black" arrowhead="normal"];
"split" -> "p_b" [color="black" arrowhead="normal"];
"join" -> "end" [color="black" arrowhead="normal"];
"p_b_end" -> "join" [color="black" arrowhead="normal"];
"p_a" -> "t_a" [color="black" arrowhead="normal"];
}